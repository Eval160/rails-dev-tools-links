<h1>Resources#edit</h1>
<%= simple_form_for @resource do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :url %>
  <%= f.association :categories, as: :check_boxes, collection: Category.order("name"), wrapper: :vertical_collection_inline %>
  <%= f.button :submit %>
<% end %>
<%= link_to "Back", :back %>
